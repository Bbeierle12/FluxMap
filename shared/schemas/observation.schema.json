{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Observation",
  "type": "object",
  "properties": {
    "source": { "type": "string" },
    "macAddress": { "type": "string" },
    "ipAddress": { "type": "string" },
    "hostname": { "type": "string" },
    "vendor": { "type": "string" },
    "typeHint": { "type": "string" },
    "serviceHint": { "type": "string" },
    "observedAtUtc": { "type": "string", "format": "date-time" }
  },
  "required": ["source"]
}
